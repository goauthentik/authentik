{
    "name": "@goauthentik/tsconfig",
    "version": "1.0.7",
    "description": "authentik's base TypeScript configuration.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/goauthentik/authentik.git",
        "directory": "packages/tsconfig"
    },
    "scripts": {
