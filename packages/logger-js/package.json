{
    "name": "@goauthentik/logger-js",
    "version": "1.0.0",
    "description": "Pino-based logger configuration for Authentik",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/goauthentik/authentik.git",
        "directory": "packages/logger-js"
    },
    "scripts": {
        "build": "tsc -p .",
        "lint": "eslint --fix .",
        "lint-check": "eslint --max-warnings 0 .",
        "prettier": "prettier --write .",
        "prettier-check": "prettier --check ."
    },
    "type": "module",
    "types": "./out/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./out/index.d.ts",
            "node": "./lib/node.js",
            "import": "./index.js",
            "browser": "./lib/browser.js"
        },
        "./browser": {
            "types": "./out/lib/browser.d.ts",
            "import": "./lib/browser.js"
        },
        "./node": {
            "types": "./out/lib/node.d.ts",
            "import": "./lib/node.js"
        }
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@goauthentik/eslint-config": "../eslint-config",
        "@goauthentik/tsconfig": "../tsconfig",
        "@types/node": "^25.0.0",
        "@typescript-eslint/eslint-plugin": "^8.49.0",
        "@typescript-eslint/parser": "^8.49.0",
        "pino": "^10.3.1",
        "pino-pretty": "^13.1.2",
        "eslint": "^9.39.1",
        "prettier": "^3.8.1",
        "prettier-plugin-packagejson": "^3.0.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.49.0"
    },
    "peerDependencies": {
        "pino": "^10.3.1",
        "pino-pretty": "^13.1.2"
    },
    "peerDependenciesMeta": {
        "pino": {
            "optional": true
        },
        "pino-pretty": {
            "optional": true
        }
    },
    "files": [
        "./index.js",
        "lib/**/*",
        "out/**/*"
    ],
    "engines": {
        "node": ">=24",
        "npm": ">=11.10.1"
    },
    "devEngines": {
        "runtime": {
            "name": "node",
            "version": ">=24",
            "onFail": "warn"
        },
        "packageManager": {
            "name": "npm",
            "version": ">=11.10.1",
            "onFail": "warn"
        }
    },
    "packageManager": "npm@11.10.1+sha512.c286afb98d8e803150d4afed07d407b14b96f7cf6729fbec29337f879a86c92fb7f8186f5cdfc30364cdcf1e7af496857d3aab113e5b110347c1584593e53582",
    "prettier": "./index.js",
    "overrides": {
        "format-imports": {
            "eslint": "$eslint"
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
