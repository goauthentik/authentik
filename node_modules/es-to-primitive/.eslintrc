{
	"root": true,

	"extends": "@ljharb",

	"rules": {
		"func-name-matching": 0,
		"id-length": [2, { "min": 1, "max": 24, "properties": "never" }],
		"new-cap": [2, { "capIsNewExceptions": ["GetMethod"] }]
	},

	"overrides": [
		{
			"files": "test/**",
			"rules": {
				"max-lines-per-function": 0,
			},
		}
	],
}
